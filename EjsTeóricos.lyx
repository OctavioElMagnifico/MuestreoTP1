\begin_layout Standard
\begin_inset Formula 
\[
d_{i}=\overline{y}_{U_{i}}-\overline{y}_{U}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $W_{h}=\nicefrac{N_{h}}{N}\Rightarrow\overline{y}_{U}=\sum_{h=1}^{H}W_{h}\overline{y}_{U_{h}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
d_{i}=\overline{y}_{U_{i}}-\sum_{h=1}W_{h}\overline{y}_{U_{h}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
d_{i}=\left(1-W_{i}\right)\overline{y}_{U_{i}}-\sum_{h\neq i}W_{h}\overline{y}_{U_{h}}
\]

\end_inset


\end_layout

\begin_layout Standard
En Särndall, probamos que
\lang spanish
 siempre se cumple 
\begin_inset Formula $\mathbb{E}\left(\sum_{s}\check{a}_{k}\right)=\sum_{U}a_{k}$
\end_inset

.
 
\end_layout

\begin_layout Standard

\lang spanish
Es decir, el estimador de HT es insertable.
 
\end_layout

\begin_layout Standard

\lang spanish
Sean 
\begin_inset Formula $\hat{\overline{y}}_{s_{h}}=\frac{1}{N_{h}}\sum_{s\cap U_{h}}\frac{y_{h}}{\pi_{h}}=\frac{1}{N_{h}}\sum_{s\cap U_{h}}\check{y}_{h}$
\end_inset


\end_layout

\begin_layout Standard

\lang spanish
\begin_inset Formula $\pi_{h}=\frac{n_{h}}{N_{h}}$
\end_inset


\end_layout

\begin_layout Itemize
\begin_inset Formula 
\[
\hat{d}_{i\pi}=\left(1-W_{i}\right)\hat{\overline{y}}_{s_{i}}-\sum_{h\neq i}W_{h}\hat{\overline{y}}_{s_{h}}
\]

\end_inset


\end_layout

\begin_layout Standard
Probamos que es un estimador de la variable deseada (e insesgado).
 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left(\hat{d}_{i\pi}\right)=\mathbb{E}\left[\left(1-W_{i}\right)\hat{\overline{y}}_{s_{i}}-\sum_{h\neq i}W_{h}\hat{\overline{y}}_{s_{h}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left(\hat{d}_{i\pi}\right)=\left(1-W_{i}\right)\mathbb{E}\left(\hat{\overline{y}}_{s_{i}}\right)-\sum_{h\neq i}W_{h}\mathbb{E}\left(\hat{\overline{y}}_{s_{h}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
Por cómo están definidos los 
\begin_inset Formula $\hat{\overline{y}}_{s_{h}}$
\end_inset

, el resultado de Särndall aplica inmediatamente.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula 
\[
\mathbb{E}\left(\hat{d}_{i\pi}\right)=\left(1-W_{i}\right)\overline{y}_{U_{i}}-\sum_{h\neq i}W_{h}\overline{y}_{U_{h}}
\]

\end_inset


\end_layout

\begin_layout Subsection*
ii Desarollar la varianza de 
