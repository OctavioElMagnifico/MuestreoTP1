\end_layout

\begin_layout Standard
\begin_inset Formula $S_{B}'=\left\{ b'_{ij}/b'_{ij}\in S_{B}\wedge b'_{ij}\setminus a_{ij}=y_{1}\wedge a_{ij}\setminus b'_{ij}=y_{N}\right\} $
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $S_{B}'=S_{B}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
