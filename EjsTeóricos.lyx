\begin_layout Standard
\begin_inset Formula 
\[
\sum_{s\in S_{A}}\overline{y}_{\pi}\left(s\right)-\sum_{s\in S_{B}}\overline{y}_{\pi}\left(s\right)=\sum_{a_{i}\in S_{A}}\sum_{j=1}^{n}\frac{a_{ij}}{n}-\sum_{b_{i}\in S_{B}}\sum_{j=1}^{n}\frac{b_{ij}}{n}
\]

\end_inset


\end_layout

\begin_layout Standard
