\begin_layout Standard

\lang spanish
\begin_inset Formula 
\[
V_{EST,Ney}\left(\hat{t}_{\pi}\right)=\frac{N_{1}^{2}}{n\frac{N_{1}S_{yU_{1}}}{N_{1}S_{yU_{1}}+N_{2}S_{yU_{2}}}}S_{yU_{1}}^{2}+\frac{N_{2}^{2}}{n\frac{N_{2}S_{yU_{2}}}{N_{1}S_{yU_{1}}+N_{2}S_{yU_{2}}}}S_{yU_{2}}^{2}
